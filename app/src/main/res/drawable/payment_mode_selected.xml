<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#25AE88" />
            <corners android:radius="@dimen/space_4" />
            <solid android:color="#7ED321" />

            <size
                android:width="@dimen/space_70"
                android:height="@dimen/space_50" />
        </shape>
    </item>

    <item
        android:drawable="@drawable/circular_tick"
        android:gravity="top|center_horizontal"
        android:top="@dimen/space_5"/>
</layer-list>