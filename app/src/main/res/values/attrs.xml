<?xml version="1.0" encoding="utf-8"?>
<resources>

  <!-- Defines the custom XML attributes supported for a HorizontalListView -->

  <declare-styleable name="CustomFontTextView">
    <attr name="fontName" format="string" />
  </declare-styleable>

  <declare-styleable name="RelatedArticlesComponent">
    <attr name="articleTitle" format="string" localization="suggested" />
    <attr name="articleImage" format="integer" />
  </declare-styleable>

  <declare-styleable name="PollChoiceComponents">
    <attr name="pollChoiceText" format="string" localization="suggested" />
  </declare-styleable>

  <declare-styleable name="ImagePollChoiceComponents">
    <attr name="pollOptionImage" format="integer" />
  </declare-styleable>

  <declare-styleable name="SearchArticlesComponent">
    <attr name="searchArticleTitle" format="string" localization="suggested" />
    <attr name="searchArticleDesc" format="string" localization="suggested" />
    <attr name="searchArticleImage" format="integer" />
  </declare-styleable>

  <declare-styleable name="RangeBar">
    <attr name="tickStart" format="float" />
    <attr name="tickEnd" format="float" />
    <attr name="tickInterval" format="float" />
    <attr name="tickHeight" format="dimension" />
    <attr name="tickColor" format="color" />
    <attr name="barWeight" format="dimension" />
    <attr name="rangeBarColor" format="reference|color" />
    <attr name="textColor" format="reference|color" />
    <attr name="pinColor" format="reference|color" />
    <attr name="selectorSize" format="dimension" />
    <attr name="selectorBoundaryColor" format="reference|color" />
    <attr name="selectorBoundarySize" format="dimension" />
    <attr name="pinPadding" format="dimension" />
    <attr name="pinMinFont" format="dimension" />
    <attr name="pinMaxFont" format="dimension" />
    <attr name="rangeBarPaddingBottom" format="dimension" />
    <attr name="selectorColor" format="reference|color" />
    <attr name="rangeBar" format="boolean" />
    <attr name="temporaryPins" format="boolean" />
    <attr name="connectingLineWeight" format="dimension" />
    <attr name="connectingLineColor" format="reference|color" />
    <attr name="pinRadius" format="dimension" />
  </declare-styleable>

  <declare-styleable name="RoundedHorizontalProgress">
    <attr name="backgroundColor" format="color" />
    <attr name="progressColor" format="color" />
    <attr name="isRounded" format="boolean" />
  </declare-styleable>

  <declare-styleable name="ResizableTextView">
    <attr name="maxLines" format="integer" />
  </declare-styleable>

  <declare-styleable name="ShareButtonWidget">
    <attr name="shareText" format="string" localization="suggested" />
    <attr name="shareImage" format="integer" />
    <attr name="shareBgColor" format="reference|color" />
    <attr name="shareBgDrawable" format="reference" />
    <attr name="shareImageTint" format="reference|color" />
    <attr name="gravity">
      <flag name="bottom" value="80" />
      <flag name="center" value="17" />
      <flag name="center_horizontal" value="1" />
      <flag name="center_vertical" value="16" />
      <flag name="clip_horizontal" value="8" />
      <flag name="clip_vertical" value="128" />
      <flag name="end" value="8388613" />
      <flag name="fill" value="119" />
      <flag name="fill_horizontal" value="7" />
      <flag name="fill_vertical" value="112" />
      <flag name="left" value="3" />
      <flag name="right" value="5" />
      <flag name="start" value="8388611" />
      <flag name="top" value="48" />
    </attr>
    <attr name="radius" format="dimension" />
    <attr name="elevation" format="dimension" />
    <attr name="textSize" format="dimension" />
    <attr name="shareTextColor" format="reference|color" />
    <attr name="borderColor" format="reference|color" />
    <attr name="borderthickness" format="dimension" />
    <attr name="textStyle">
      <flag name="normal" value="0" />
      <flag name="bold" value="1" />
      <flag name="italic" value="2" />
    </attr>
  </declare-styleable>

  <declare-styleable name="IndefinitePagerIndicator">
    <attr name="dotCount" format="integer" />
    <attr name="fadingDotCount" format="integer" />
    <attr name="dotRadius" format="dimension" />
    <attr name="selectedDotRadius" format="dimension" />
    <attr name="dotColor" format="color" />
    <attr name="selectedDotColor" format="color" />
    <attr name="dotSeparation" format="dimension" />
    <attr name="supportRTL" format="boolean" />
    <attr name="verticalSupport" format="boolean" />
  </declare-styleable>

  <declare-styleable name="MomspressoButtonWidget">

    <!-- Default Android attrs -->
    <attr name="android:enabled" />
    <attr name="android:text" />
    <attr name="android:textSize" />
    <attr name="android:textAllCaps" />
    <attr name="android:textStyle" />
    <attr name="android:fontFamily" />

    <attr name="fb_defaultColor" format="color" />
    <attr name="fb_text" format="string" />

    <attr name="fb_textColor" format="color" />
    <attr name="fb_iconColor" format="color" />

    <attr name="fb_textFont" format="string" />
    <attr name="fb_textFontRes" format="reference" />
    <attr name="fb_iconFont" format="string" />

    <attr name="fb_textSize" format="dimension" />
    <attr name="fb_iconResource" format="integer" />
    <attr name="fb_fontIconResource" format="string" />
    <attr name="fb_fontIconSize" format="dimension" />
    <attr name="fb_iconPosition" format="enum">
      <enum name="left" value="1" />
      <enum name="right" value="2" />
      <enum name="top" value="3" />
      <enum name="bottom" value="4" />
    </attr>

    <attr name="fb_textPosition" format="enum">
      <enum name="left" value="1" />
      <enum name="right" value="2" />
      <enum name="top" value="3" />
      <enum name="bottom" value="4" />
    </attr>

    <attr name="fb_textGravity" format="enum">
      <enum name="top" value="0x30" />
      <enum name="bottom" value="0x50" />
      <enum name="left" value="0x03" />
      <enum name="right" value="0x05" />
      <enum name="center_vertical" value="0x10" />
      <enum name="fill_vertical" value="0x70" />
      <enum name="center_horizontal" value="0x01" />
      <enum name="fill_horizontal" value="0x07" />
      <enum name="center" value="0x11" />
      <enum name="fill" value="0x77" />
      <enum name="clip_vertical" value="0x80" />
      <enum name="clip_horizontal" value="0x08" />
      <enum name="start" value="0x00800003" />
      <enum name="end" value="0x00800005" />
    </attr>

    <attr name="fb_iconPaddingLeft" format="dimension" />
    <attr name="fb_iconPaddingRight" format="dimension" />
    <attr name="fb_iconPaddingTop" format="dimension" />
    <attr name="fb_iconPaddingBottom" format="dimension" />

    <attr name="fb_radiusTopLeft" format="dimension" />
    <attr name="fb_radiusTopRight" format="dimension" />
    <attr name="fb_radiusBottomLeft" format="dimension" />
    <attr name="fb_radiusBottomRight" format="dimension" />

    <attr name="fb_borderColor" format="color" />
    <attr name="fb_borderWidth" format="dimension" />
    <attr name="fb_focusColor" format="color" />
    <attr name="fb_disabledColor" format="color" />
    <attr name="fb_disabledTextColor" format="color" />
    <attr name="fb_disabledBorderColor" format="color" />
    <attr name="fb_radius" format="dimension" />
    <attr name="fb_textAllCaps" format="boolean" />

    <attr name="fb_ghost" format="boolean" />
    <attr name="fb_useSystemFont" format="boolean" />
  </declare-styleable>

  <attr name="mentionTextColor" format="color" />
  <attr name="mentionTextBackgroundColor" format="color" />
  <attr name="selectedMentionTextColor" format="color" />
  <attr name="selectedMentionTextBackgroundColor" format="color" />

  <declare-styleable name="MentionsEditText">
    <attr name="mentionTextColor" />
    <attr name="mentionTextBackgroundColor" />
    <attr name="selectedMentionTextColor" />
    <attr name="selectedMentionTextBackgroundColor" />
  </declare-styleable>

  <declare-styleable name="RichEditorView">
    <attr name="mentionTextColor" />
    <attr name="mentionTextBackgroundColor" />
    <attr name="selectedMentionTextColor" />
    <attr name="selectedMentionTextBackgroundColor" />
  </declare-styleable>
  <!--<declare-styleable name="KidsInfoComponent">--><!--<attr name="searchArticleTitle" format="string" localization="suggested" />--><!--<attr name="searchArticleDesc" format="string" localization="suggested" />--><!--<attr name="searchArticleImage" format="integer" />--><!--</declare-styleable>-->
</resources>